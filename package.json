{
  "name": "ahemkek",
  "version": "0.0.1",
  "description": "A chrome extension that uses the things to do things.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/staccDOTsols/chrome-extension-boilerplate-react.git"
  },
  "files": [
    "pages",
    "public",
    "styles",
    ".eslintrc.json",
    "LICENSE",
    "next.config.js",
    "next-env.d.ts",
    "README.md",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prestart": "npm-link-shared ../common/node_modules/ . react",
    "start2": "next dev",
    "build2": "next build",
    "export": "next export -o ./build/web",
    "start:prod": "next start",
    "test": "jest --passWithNoTests",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "deploy:ar": "yarn export && arweave deploy-dir  ../../build/web --key-file ",
    "deploy:gh": "gh-pages -d .next --repo git@github.com:luvfreedom/react-strata-nextjs-starter.git -t true",
    "deploy": "cross-env  yarn build && yarn deploy:gh",
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.1",
    "@hot-loader/react-dom": "^17.0.2",
    "@metaplex-foundation/mpl-token-metadata": "^1.1.0",
    "@solana/wallet-adapter-react": "^0.15.3",
    "@solana/wallet-adapter-react-ui": "^0.9.1",
    "@strata-foundation/react": "3.2.3",
    "gh-pages": "^3.2.3",
    "next": "12.0.7",
    "npm-link-shared": "^0.5.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-hot-toast": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/chrome": "^0.0.177",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.6.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.5.1",
    "sass-loader": "^12.4.0",
    "shx": "^0.3.3",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "~4.4.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
